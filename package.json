{
  "name": "svelte-poc",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "preview:docker": "PORT=3000 svelte-kit preview --host 0.0.0.0",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "fmt": "dprint fmt",
    "eslint": "eslint --fix --ignore-path .gitignore .",
    "check:all": "yarn fmt && yarn eslint"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1"
  },
  "devDependencies": {
    "@carbon/charts-svelte": "^0.51.3",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "~5.4.0",
    "@typescript-eslint/parser": "~5.4.0",
    "carbon-components-svelte": "^0.49.0",
    "carbon-icons-svelte": "^10.38.0",
    "carbon-pictograms-svelte": "^12.0.0",
    "carbon-preprocess-svelte": "^0.6.0",
    "clipboard-copy": "^4.0.1",
    "cross-env": "^7.0.3",
    "d3": "^7.1.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.1",
    "svelte-check": "^2.2.9",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "~4.4.4"
  }
}
